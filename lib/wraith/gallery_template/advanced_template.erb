<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <title>Wraith // Gallery</title>

  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">

  <!-- Ionicons -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
  <!-- jvectormap -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jvectormap/1.2.2/jquery-jvectormap.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/admin-lte/2.3.2/css/AdminLTE.min.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/admin-lte/2.3.2/css/skins/_all-skins.min.css">

  <script type="text/javascript" src="http://code.jquery.com/jquery-2.2.0.min.js"></script>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.6/js/bootstrap.min.js"></script>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery.cycle/3.03/jquery.cycle.all.min.js"></script>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/unveil/1.3.0/jquery.unveil.min.js"></script>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jQuery-slimScroll/1.3.7/jquery.slimscroll.min.js"></script>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/admin-lte/2.3.2/js/app.min.js"></script>
  <style type="text/css">

    /* Navigation */
    .affix.main-header {
      -webkit-box-shadow: 0 0 10px rgba(0,0,0,0.15);
      box-shadow: 0 0 10px rgba(0,0,0,0.15);
    }

    /* Page List */
    .nav-stacked>li.active>a,
    .nav-stacked>li.active>a:hover {
      background: #f1f1f1;
    }
    .nav-stacked>li>a>input[type='checkbox'] {
      margin: 0 10px 0 5px;
    }
    .nav-stacked .text {
      text-overflow: ellipsis;
      width: 75%;
      display: inline-block;
      vertical-align: bottom;
      overflow: hidden;
      white-space: nowrap;
      font-weight: bold;
      margin-left: 5px;
    }

    .checked .check-mark:after {
      color: rgba(128,128,128,1);
    }
    .check-mark {
      word-wrap: break-word;
      width: 95%;
      display: block;
    }
    .check-mark:after {
      content:"\f00c";
      font-family: "FontAwesome";
      color: rgba(128,128,128,0.3);
      position: absolute;
      right: 65px;
      bottom: 10px;
    }
    .checked a {
      color: #000;
    }

    /* Slideshow */
    .slideshow, .slide {
      height: 500px;
      overflow: hidden;
    }
    .slide {
      background-color: #FFF !important;
      width: 100% !important;
      margin: 0;
    }

    /* Gallery */
    .compare-modal {
      position:fixed;
      margin:0;
      top:0;
      left:0;
      width: 100%;
      height: 100%;
      background:rgba(0,0,0,0.7);
      overflow: scroll;
      z-index: 1000;
    }
    .compare-container {
      padding-bottom: 300px;
      z-index: 1000;
      margin-top: 20px;
      text-align: center;
    }
    .compare-container img{
      margin-right: 20px;
      max-width: 30%;
    }
    #screenshot.affix {
      right: 0;
      top: 65px;
    }

    #screenshot .box {
      transition: all 0.3s linear;
    }
    #screenshot.affix .box {
      box-shadow: 0 0 10px rgba(0,0,0,0.15);
    }
    .lazy {
      display: inline-block;
      max-width: 100%;
      background-repeat: no-repeat;
      background-position: center;
      background-image: url('data:image/gif;base64,');
    }

    .dl-horizontal{
      word-wrap: break-word;
    }
    .modal {
      z-index: 10050;
    }
    .modal-backdrop {
      z-index: 10040;
    }
    .modal-xl {
      width: auto;
      margin: 10px;
    }

    a.thumbnail {
      cursor: pointer;
    }

    .fixed .wrapper {
      overflow: auto;
    }

  </style>
  <script type="text/javascript">
      $(function() {

          // Slideshow
          $('.slideshow').cycle({
              fx:     'fade',
              speed:  300,
              prev:   '.prev',
              next:   '.next',
              before: function (curr, next, opts) {
                  $('.active').removeClass('active');
                  var path = $(next).find('.path').attr('name');
                  $.each($('#page-list a'), function() {
                      if ($(this).attr('href').substring(1) == path) {
                          $(this).addClass('checked');
                          $(this).parent().addClass('active');
                      }
                  });
                  var $slide = ($(this)),
                      $modal = $('#compareModal'),
                      elements = getElementsFromSlider($slide);

                  renderCompareModal($modal, elements);

              },
              timeout: 0
          });

          // Keys
          $(window).keydown(function(event){
              switch (event.keyCode) {
                  case 13:
                      if (!$('body').hasClass('modal-open')) {
                          var currSlideId = $('.slideshow').data("cycle.opts").currSlide;
                          var $currSlide = $('.slideshow .row:eq(' + currSlideId + ')');
                          $currSlide.find('button').click();
                      }
                      break;
                  case 37:
                  case 38:
                      $('#page-list>li.active').prev().find('a').click();
                      return false;
                      break;
                  case 39:
                  case 40:
                      $('#page-list>li.active').next().find('a').click();
                      return false;
                      break;
              }
          });

          // Page list
          $('#page-list a').unbind().on('click', function(){
              var href = $(this).attr('href').substring(1);
              $('.slideshow').cycle($('.slide.'+href+':first').index());
          });

          // Filter pages
          $('#pagesAll a').on('click', function(){
              $('#page-list .list-group-item').show();
          });
          // Success
          $('#pagesSuccess a').on('click', function(){
              $('#page-list .list-group-item').hide();
              $('#page-list .list-group-item.list-group-item-success').show();
          });
          // Warning
          $('#pagesWarning a').on('click', function(){
              $('#page-list .list-group-item').hide();
              $('#page-list .list-group-item.list-group-item-warning').show();
          });
          // Danger
          $('#pagesDanger a').on('click', function(){
              $('#page-list .list-group-item').hide();
              $('#page-list .list-group-item.list-group-item-danger').show();
          });

          // Activate tooltip
          $('[data-toggle="tooltip"]').tooltip();


          /**
           * renderCompareModal
           *
           * @param $modal
           * @param elements
           */
          function renderCompareModal($modal, elements) {
              $modal.find('#compareModalLabel span:first').text(elements['pageTitle']);
              $modal.find('#compareModalLabel span:last').text(elements['resolution']);
              $modal.find('.firstImage').attr('src', elements['firstImage']);
              $modal.find('.secondImage').attr('src', elements['secondImage']);
              $modal.find('.diffImage').attr('src', elements['diffImage']);
              $modal.find('.firstImage').parent().attr('href', elements['firstImageUrl']);
              $modal.find('.secondImage').parent().attr('href', elements['secondImageUrl']);
          }

          /**
           * getElementsFromSlider
           *
           * @param $slide
           * @returns {Array}
           */
          function getElementsFromSlider($slide) {

              var elements = [];

              elements['pageTitle'] = $slide.find('button').data('page-title');
              elements['resolution'] = $slide.find('h4>small>span').text();
              elements['firstImage'] = $slide.find('a.thumbnail:eq(0)').attr('data-href');
              elements['secondImage'] = $slide.find('a.thumbnail:eq(1)').attr('data-href');
              elements['diffImage'] = $slide.find('a.thumbnail:eq(2)').attr('data-href');
              elements['firstImageUrl'] = $slide.find('a.thumbnail:eq(0)').next().find('a').attr('href');
              elements['secondImageUrl'] = $slide.find('a.thumbnail:eq(1)').next().find('a').attr('href');

              return elements;

          }

          // Compare modal
          $('#compareModal').on('show.bs.modal', function (event) {
              var $button = $(event.relatedTarget),
                  $modal = $(this),
                  $slide = $button.closest('.slide'),
                  elements = getElementsFromSlider($slide);

              renderCompareModal($modal, elements);

          });

          // Image modal
          $('#imageModal').on('show.bs.modal', function (event) {
              var $button = $(event.relatedTarget),
                  $modal = $(this);
              var pageTitle = $button.data('page-title'); // Extract info from data-* attributes
              var firstImage = $button.data('href'),
                  firstImageUrl = $button.next().find('a').attr('href');

              $modal.find('#imageModalLabel').text(pageTitle);
              $modal.find('.firstImage').attr('src', firstImage);
              $modal.find('.firstImage').parent().attr('href', firstImageUrl);
          });

          // Affix
          $('#screenshot').affix({
              offset: {
                  top: function () {
                      navigationSectionHeight = $('#navigationSection').outerHeight(true);
                      overviewSectionHeight = $('#overviewSection').outerHeight(true);
                      return (this.top =  navigationSectionHeight + overviewSectionHeight + 65);
                  },
                  bottom: 0
              }
          });

          $('#navigationSection').affix({
              offset: {
                  top: 0,
                  bottom: 0
              }
          });

          // lazy load
          // TODO: Bind event to slideshow
          $("img.lazy").unveil(100, function() {
              $(this).load(function() {
                  this.style.background = '#fff';
              });
          });

          $(".box").activateBox();

          $('.carousel').carousel();

          window.setTimeout( function(){
              $('.overlay').hide();
          }, 1000 );


          // Hash URL
          var hash = $(location).attr('hash');
          if (hash !== "") {
              var hashElement = "#page-list a[href^='" + hash +  "']";
              $(hashElement).click();

              $('html, body').animate({
                  scrollTop:$(hashElement).offset().top -($('#navigationSection').outerHeight() +80)
              },'slow');
          }

          $('#page-list>li>a').on('click', function(){
              $('html, body').animate({
                  scrollTop:$(this).offset().top -($('#navigationSection').outerHeight() +83)
              },100);
          });
      });

  </script>

</head>
<%
  # Count pages
  totalPagesCount = 0
  failedPagesCount = 0
  warningPagesCount = 0
  passedPagesCount = 0

  directories.each do |dir, sizes|
    sizes.to_a.sort.each do |size, files|
      totalPagesCount += 1
      if files[:data] > threshold
        failedPagesCount += 1
      else
        if files[:data] < threshold and files[:data] != 0
          warningPagesCount += 1
        else
          passedPagesCount += 1
        end
      end
    end
  end

  # Progress bar
  percentFailed = failedPagesCount.to_f / totalPagesCount * 100
  percentWarning = warningPagesCount.to_f / totalPagesCount * 100
  percentPassed = passedPagesCount.to_f / totalPagesCount * 100

  # Status label
  statusLabel = ''

  if failedPagesCount > 0
    statusLabel = 'label-danger'
    statusText = 'Failed'
  else if warningPagesCount > 0
         statusLabel = 'label-warning'
         statusText = 'Warning'
       else
         statusLabel = 'label-success'
         statusText = 'Passed'
       end
  end
%>
<body class="hold-transition skin-black layout-top-nav fixed">
<div class="wrapper">

  <!-- Header section begin -->
  <header id="navigationSection" class="main-header">
    <nav class="navbar navbar-static-top">
      <div class="container-fluid">
        <div class="navbar-header">
          <a href="#" class="navbar-brand"><b>Wraith</b></a>
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse">
            <i class="fa fa-bars"></i>
          </button>
        </div>
      </div>
      <!-- /.container-fluid -->
    </nav>
  </header>
  <!-- Header section end -->

  <!-- Content wrapper begin -->
  <div class="content-wrapper">

    <section class="content-header">
      <h1>
        Overview
        <small><%=config['directory']%></small>
      </h1>
    </section>

    <div class="content">

      <!-- Overview section begin -->
      <section id="overviewSection">

        <!-- Summary begin -->
        <div class="row">
          <div class="col-sm-6">
            <div class="box box-info">
              <div class="box-header with-border">
                <i class="ion ion-ios-settings"></i>
                <h3 class="box-title">Project Configuration</h3>
                <div class="box-tools pull-right">
                  <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse"><i class="fa fa-minus"></i></button>
                </div>
              </div>
              <!-- /.box-header -->
              <div class="box-body">
                <dl class="dl-horizontal">
                  <dt>Domains</dt>
                  <% config['domains'].each do |domain| %>
                      <dd><span class="text-muted"><%=domain[1]%></span></dd>
                  <% end %>
                  <dt>Directory</dt>
                  <dd><span class="text-muted"><%=config['directory']%></span></dd>
                  <dt>Generated</dt>
                  <dd><span class="text-muted"><%=Time.now.strftime('%Y/%m/%d %H:%M:%S')%></span></dd>
                  <dt>Threshold</dt>
                  <dd><span class="text-muted"><%=config['threshold']%>%</span></dd>
                  <dt>Fuzz</dt>
                  <dd><span class="text-muted"><%=config['fuzz']%></span></dd>
                  <dt>Resolution</dt>
                  <dd><span class="text-muted"><%=config['screen_widths']%></span></dd>
                  <dt>Spider Skips</dt>
                  <dd><span class="text-muted"><%=config['spider_skips'] ? config['spider_skips'] : '-'%></span></dd>
                  <dt>Before Capture</dt>
                  <dd><span class="text-muted"><%=config['before_capture'] ? config['before_capture'] : '-'%></span></dd>
                  <dt>Browser</dt>
                  <dd><span class="text-muted"><%=config['browser']%></span></dd>
                  <dt>Status</dt>
                  <dd><span id="status" class="label <%=statusLabel%> text-uppercase"><%=statusText%></span></dd>
                </dl>
              </div>
              <!-- /.box-body -->
              <div class="overlay">
                <i class="fa fa-refresh fa-spin"></i>
              </div>
            </div>
          </div>
          <div class="col-md-6 col-sm-6">
            <div class="info-box bg-green">
              <span class="info-box-icon"><i class="fa fa-check"></i></span>
              <div class="info-box-content">
                <span class="info-box-text">Passed</span>
                <span class="info-box-number"><%=passedPagesCount%> / <%=totalPagesCount%></span>
                <div class="progress">
                  <div class="progress-bar" style="width: <%=percentPassed.round%>%"></div>
                </div>
                <span class="progress-description"><%=percentPassed.round%>%</span>
              </div><!-- /.info-box-content -->
            </div><!-- /.info-box -->
            <div class="info-box bg-yellow">
              <span class="info-box-icon"><i class="fa fa-exclamation"></i></span>
              <div class="info-box-content">
                <span class="info-box-text">Warning</span>
                <span class="info-box-number"><%=warningPagesCount%> / <%=totalPagesCount%></span>
                <div class="progress">
                  <div class="progress-bar" style="width: <%=percentWarning.round%>%"></div>
                </div>
                <span class="progress-description"><%=percentWarning.round%>%</span>
              </div><!-- /.info-box-content -->
            </div><!-- /.info-box -->
            <div class="info-box bg-red">
              <span class="info-box-icon"><i class="fa fa-bolt"></i></span>
              <div class="info-box-content">
                <span class="info-box-text">Failed</span>
                <span class="info-box-number"><%=failedPagesCount%> / <%=totalPagesCount%></span>
                <div class="progress">
                  <div class="progress-bar" style="width: <%=percentFailed.round%>%"></div>
                </div>
                <span class="progress-description"><%=percentFailed.round%>%</span>
              </div><!-- /.info-box-content -->
            </div><!-- /.info-box -->
          </div>

        </div>
        <!-- Summary end -->

        <!-- Progress bar begin -->
        <div class="progress">
          <div class="progress-bar progress-bar-success" style="width: <%=percentPassed%>%">
            <span class="text-uppercase">Passed <%=percentPassed.round%>%</span>
          </div>
          <div class="progress-bar progress-bar-warning" style="width: <%=percentWarning%>%">
            <span class="text-uppercase">Warning <%=percentWarning.round%>%</span>
          </div>
          <div class="progress-bar progress-bar-danger" style="width: <%=percentFailed%>%">
            <span class="text-uppercase">Failed <%=percentFailed.round%>%</span>
          </div>
        </div>
        <!-- Progress bar end -->

      </section>
      <!-- Overview section end -->

      <!-- Detail section begin -->
      <section id="detailSection">

        <h2 class="page-header">Detail</h2>

        <div class="row">

          <!-- Pages panels begin -->
          <div class="col-sm-12 col-md-6">
            <div class="box box-default">
              <div class="box-header with-border">
                <i class="ion ion-clipboard"></i>
                <h3 class="box-title">Pages</h3>
                <div class="box-tools pull-right">
                  <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse"><i class="fa fa-minus"></i></button>
                </div>
              </div>
              <div class="box-body no-padding">
                <ul id="page-list" class="nav nav-stacked">

                  <% directories.each do |dir, sizes| %>
                  <% sizes.to_a.sort.each do |size, files| %>
                  <% if files[:data] > threshold %>
                  <li>
                    <a href="#<%=path%><%=dir%>_<%=size%>">
                      <span class="pull-right badge bg-red"><%=files[:data].ceil%>%</span>
                      <span class="check-mark"></span>
                      <span class="label label-default"><%=size%>px</span>
                      <span class="text"><%=dir.gsub('__', '/')%></span>
                    </a>
                  </li>
                  <% else %>
                  <% if files[:data] < threshold and files[:data] != 0 %>
                  <li>
                    <a href="#<%=path%><%=dir%>_<%=size%>">
                      <span class="pull-right badge bg-yellow"><%=files[:data].ceil%>%</span>
                      <span class="check-mark"></span>
                      <span class="label label-default"><%=size%>px</span>
                      <span class="text"><%=dir.gsub('__', '/')%></span>
                    </a>
                  </li>
                  <% else %>
                  <li>
                    <a href="#<%=path%><%=dir%>_<%=size%>">
                      <span class="pull-right badge bg-green"><%=files[:data].ceil%>%</span>
                      <span class="check-mark"></span>
                      <span class="label label-default"><%=size%>px</span>
                      <span class="text"><%=dir.gsub('__', '/')%></span>
                    </a>
                  </li>
                  <% end %>
                  <% end %>
                  <% end %>
                  <% end %>
                </ul>
              </div>
            </div>
          </div>
          <!-- Pages panels end -->

          <!-- Sceenshot panels begin -->
          <div id="screenshot" class="hidden-xs hidden-sm col-md-6">

            <div class="box box-default">
              <div class="box-header with-border">
                <i class="ion ion-images"></i>
                <h3 class="box-title">Screenshots</h3>
                <div class="box-tools pull-right">
                  <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse"><i class="fa fa-minus"></i></button>
                </div>
              </div>
              <div class="box-body">
                <div class="container-fluid">
                  <div class="row">
                    <div class="slideshow pull-left col-sm-12">
                      <% directories.each do |dir, sizes| %>
                          <% sizes.to_a.sort.each do |size, files| %>
                              <div class="row slide <%=dir%>_<%=size%>">
                                <a class="path" name="<%=dir%>_<%=size%>"></a>
                                <div class="col-sm-12">
                                  <h4>
                                    Resolution: <small><span class="label label-default"><%=size%>px</span></small>
                                    <div class="pull-right">
                                      <% if files[:data] > threshold %>
                                          <span class="label label-danger text-uppercase">Failed</span>
                                      <% else %>
                                          <% if files[:data] < threshold and files[:data] != 0 %>
                                              <span class="label label-warning text-uppercase">Warning</span>
                                          <% else %>
                                              <span class="label label-success text-uppercase">Passed</span>
                                          <% end %>
                                      <% end %>
                                      <span class="label label-default text-uppercase"><%=files[:data]%>% Change</span>
                                    </div>
                                  </h4>
                                  <br>
                                </div>

                                <% files[:variants].each do |variant| %>
                                    <div class="col-sm-4">
                                      <a class="thumbnail" data-toggle="modal" data-target="#imageModal" data-page-title="<%=variant[:url]%>" data-href="<%=variant[:filename]%>" title="<%=variant[:name]%>">
                                        <img class="lazy" src="" data-src="<%=variant[:thumb]%>" alt="" />
                                      </a>
                                      <p class="text-center"><a href="<%=variant[:url]%>" target="_blank"><i class="fa fa-external-link" data-toggle="tooltip" data-placement="right" title="<%=variant[:name]%>"></i></a></p>
                                    </div>
                                <% end %>
                                <div class="col-sm-4">
                                  <% if files[:diff] %>
                                      <a class="thumbnail" data-toggle="modal" data-target="#imageModal" data-page-title="<%=files[:variants][0][:url]%>" data-href="<%=files[:diff][:filename]%>">
                                        <img class="lazy" src="" data-src="<%=files[:diff][:thumb]%>" alt="" />
                                      </a>
                                  <% end %>
                                </div>
                                <div class="col-sm-12">
                                  <br>
                                  <button type="button" class="btn btn-primary btn-sm btn-block" data-toggle="modal" data-target="#compareModal" data-page-title="<%=files[:variants][0][:url]%>"><i class="ion ion-contrast"></i> Compare</button>
                                </div>
                              </div>
                          <% end %>
                      <% end %>
                    </div>
                  </div>
                </div>
              </div>
              <div class="box-footer">
                <div class="pull-right">
                  <button type="button" class="btn btn-sm btn-default prev"><i class="ion ion-reply"></i> previous</button>
                  <button type="button" class="btn btn-sm btn-default next"><i class="ion ion-forward"></i> next</button>
                </div>
              </div>
            </div>
            <!-- Sceenshot panels end -->

          </div>


        </div>
      </section>
      <!-- Detail section end -->

    </div>

    <!-- Footer section begin -->
    <footer id="footerSection" class="main-footer">
      <div class="pull-right hidden-xs">
        <b>Version</b> <%=version%>
      </div>
      <strong>Wraith</strong>
    </footer>
    <!-- Footer section end -->

  </div>
  <!-- Content wrapper end -->

</div>

<!-- Modal begin -->
<div id="compareModal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="compareModalLabel">
  <div class="modal-dialog modal-xl" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="compareModalLabel"><span>Title</span> <span class="label label-default">1280px</span></h4>
      </div>
      <div class="modal-body">
        <div class="row">
          <div class="col-sm-4">
            <a href="#" class="thumbnail" target="_blank"><img class="firstImage" src="" alt=""/></a>
          </div>
          <div class="col-sm-4">
            <a href="#" class="thumbnail" target="_blank"><img class="secondImage" src="" alt=""/></a>
          </div>
          <div class="col-sm-4">
            <img class="diffImage img-thumbnail" src="" alt=""/>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default prev"><i class="ion ion-reply"></i> previous</button>
        <button type="button" class="btn btn-default next"><i class="ion ion-forward"></i> next</button>
